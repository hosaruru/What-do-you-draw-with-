<div class='container px-5 px-sm-0'>
    <div class='row'>
        <div class='col-md-8 offset-md-1'>
        <h2>会員一覧</h2>
        <table class='table table-hover table-inverse'>
            <thead>
                <tr>
                <th>会員ID</th>
                <th>氏名</th>
                <th>メールアドレス</th>
                <th colspan="4"></th>
                </tr>
            </thead>
            <tbody>
                <% @results.each do |user| %>
                <tr>
                <td><%= user.id %></td>
                <td><%= link_to user.user_name, admin_user_path(user.id) %></td>
                <td><%= user.email %></td>
                </tr>
                <% end %>
            </tbody>
        </table>
        </div>
    </div>
</div>